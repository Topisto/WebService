	</operation>		
	<operation name="get_language">
		<messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
	</operation>		
	<operation name="get_organization">
		<messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
	</operation>	
	<operation name="get_symbol">
		<messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
	</operation>						
	<operation name="get_term">
		<messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
	</operation>	
	<operation name="search_concept">
		<messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
	</operation>		
	<operation name="search_document">
		<messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
	</operation>		
	<operation name="search_language">
		<messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
