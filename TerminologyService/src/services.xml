<?xml version="1.0" encoding="UTF-8"?>
<service  name="TerminologyService">
	<description>Provides the operations of the terminology service according to ISO29002-20</description>
	<parameter name="ServiceClass">
		InformationSearch.TerminologyServiceProvider
	</parameter>
	<operation name="get_abbreviation">
		<messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
	</operation>		
	<operation name="get_concept">
		<messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
	</operation>	
	<operation name="get_concept_deep">
		<messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
	</operation>	
	<operation name="get_concept_equivalence_relationship">
		<messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
	</operation>	
	<operation name="get_concept_type">
		<messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
	</operation>	
	<operation name="get_definition">
		<messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
	</operation>	
	<operation name="get_document">
		<messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
	</operation>		
	<operation name="get_file_representation">
		<messageReceiver class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
	</operation>		
